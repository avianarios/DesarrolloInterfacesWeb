  "devDependencies": {
    "@parcel/transformer-sass": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.13.3",
    "parcel-plugin-imagemin": "^4.0.2",
    "rimraf": "^5.0.5",
    "sass": "^1.83.4"
  }
}
